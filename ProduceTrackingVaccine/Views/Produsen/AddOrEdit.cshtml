@model ProduceTrackingVaccine.Models.vaksin
@{
    Layout = null;
}

@using (Html.BeginForm("AddOrEdit", "Produsen", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.idVaksin)
    <div class="form-group">
        @Html.LabelFor(model => model.noRegVaksin, new { @class = "control-label" })
        @Html.EditorFor(model => model.noRegVaksin, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.noRegVaksin)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.statusVPasien, new { @class = "control-label" })
        @Html.EditorFor(model => model.statusVPasien, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.statusVPasien)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.idProdusen, new { @class = "control-label" })
        @Html.EditorFor(model => model.idProdusen, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.idProdusen)
    </div>
    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary" />
        <input type="reset" value="Reset" class="btn btn-warning" />
    </div>
}